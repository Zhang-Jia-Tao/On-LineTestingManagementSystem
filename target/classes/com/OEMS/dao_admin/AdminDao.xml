<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.OEMS.dao_admin.AdminDao">


    <select id="Admin_Login" resultType="Integer">
        select count(*) from administrators where name=#{name} and password = #{password}
    </select>

</mapper>

